{

  "manifest_version": 2,
  "name": "Bns-ext",
  "version": "1.0",

  "description": "Web Extension for bns",

  "48": {
    "icons": "static/imgs/favicon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "static/imgs/favicon.png",
    "default_title": "bns-ext",
    "default_popup": "static/index.html"
  },
  "background": {
    "page": "static/index.html"
  },
  "content_security_policy": "script-src 'unsafe-inline' 'self'"
}
